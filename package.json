{
  "name": "yaf-server",
  "version": "0.0.4",
  "repository": {
    "type": "git",
    "url": "https://github.com/falsefalse/geoip-server.git"
  },
  "private": true,
  "scripts": {
    "start": "forever -w -c 'node --inspect' ./src/server.js",
    "forever": "forever start ./src/server.js 80",
    "stop": "forever stop ./src/server.js",
    "lint": "eslint src/*.js",
    "db:load": "mkdir -p ./db && geoipupdate -f db_update.conf -d ./db -v && ls -lah db/GeoLite2-City.mmdb",
    "db:clean": "rm -fr ./db"
  },
  "dependencies": {
    "express": "^4.17.1",
    "maxmind": "^4.3.8"
  },
  "devDependencies": {
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "prettier": "^2.8.3"
  }
}
